aws.batch.maxParallelTransfers = 1

process {
    withName:'CRAM2BAM'{
        queue = "${QUEUE}"
        cpus   = 30
        memory = 100.GB
        publishDir = [
            publish_dir_mode = 'copy'
            publishDir = "${params.outdir}"
            storageClass: "${params.storageClass}",
        ]
    }
}